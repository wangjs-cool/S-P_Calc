<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#aaaaaa" />
        </shape>
    </item>

    <item
        android:left="10dp"
        android:right="10dp"
        android:bottom="16dp">
        <shape android:shape="line">
            <stroke android:width="3dp" android:color="#666666" />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:right="10dp">
        <shape android:shape="line">
            <stroke android:width="3dp" android:color="#666666" />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:top="16dp"
        android:right="10dp">
        <shape android:shape="line">
            <stroke android:width="3dp" android:color="#666666" />
        </shape>
    </item>


</layer-list>